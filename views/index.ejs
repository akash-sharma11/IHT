                        <% }); %>
                        <% airlineLogos.forEach(function(logo) { %>
                            <img src="<%= logo.src %>" alt="<%= logo.alt %>">
                        <% }); %>
						<% airlineLogos.forEach(function(logo) { %>
                            <img src="<%= logo.src %>" alt="<%= logo.alt %>">
                        <% }); %>
                        <% airlineLogos.forEach(function(logo) { %>
                            <img src="<%= logo.src %>" alt="<%= logo.alt %>">
                        <% }); %>
                    </div>
                    <div class="logo-row">
                        <% travelLogos.forEach(function(logo) { %>
